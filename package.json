{
  "name": "pwa-workshop-starter",
  "version": "0.0.1",
  "description": "A sample app showing performance gains for a simple site.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "del-cli \"dist/*\" \"!dist/.git\"",
    "build": "npm run clean && npm run build:css && npm run build:html && npm run build:icons && npm run build:img && npm run build:thumbs && npm run build:js",
    "build:css": "postcss -c postcss.config.js -o dist/assets/css/main.css src/css/main.css",
    "build:js": "webpack",
    "build:html": "cpy \"src/*.html\" dist",
    "build:icons": "cpy \"src/*.{png,xml,ico,json,svg}\" dist",
    "build:img": "imagemin src/img/* --out-dir=dist/assets/img",
    "build:thumbs": "imagemin src/img/thumbs/* --out-dir=dist/assets/img/thumbs",
    "watch:css": "watch \"npm run build:css\" src/css/",
    "watch:js": "watch \"npm run build:js\" src/js/",
    "watch:html": "watch \"npm run build:html\" src/ --ignoreDirectoryPattern=/assets/",
    "serve": "http-server dist/",
    "livereload": "livereload dist/",
    "dev": "npm run watch:css & npm run watch:js & npm run watch:html & npm run livereload & npm run serve"
  },
  "repository": "git+ssh://git@github.com/jlengstorf/pwa-workshop-starter.git",
  "keywords": [
    "pwa",
    "progressive",
    "web",
    "app",
    "lazyload",
    "sw-precache",
    "webpack"
  ],
  "author": "Jason Lengstorf <jason@lengstorf.com> (@jlengstorf)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jlengstorf/pwa-workshop-starter/issues"
  },
  "homepage": "https://github.com/jlengstorf/pwa-workshop-starter#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "cpy-cli": "^1.0.1",
    "cssnano": "^3.10.0",
    "del-cli": "^1.0.0",
    "eslint": "^3.19.0",
    "http-server": "^0.9.0",
    "imagemin-cli": "^3.0.0",
    "livereload": "^0.6.2",
    "postcss": "^5.2.17",
    "postcss-cli": "^3.1.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.1",
    "responsive-lazyload": "^1.1.6",
    "sw-precache-webpack-plugin": "^0.9.2",
    "watch": "^1.0.2",
    "webpack": "^2.4.1"
  },
  "dependencies": {}
}
